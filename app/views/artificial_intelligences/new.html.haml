.content
  %h2= t('artificial_intelligences.new')

  - form_for @artificial_intelligence do |ai|
    .field
      = ai.label :name
      = ai.text_field :name
    .field
      = ai.label :url
      = ai.text_field :url
    .field
      = ai.label :version
      = ai.text_field :version
    .field.nolabel
      = ai.submit